package tutorial.despat.bridge;

public class ChanghongTV implements ITelevision {
	
	public ChanghongTV() {
		
	}
	
	@Override
	public void on() {
		System.out.println("长虹--打开电视");
	}

	@Override
	public void off() {
		System.out.println("长虹--关闭电视");
	}

	@Override
	public void switchChannel(int channel) {
		System.out.println("长虹--换频道");
	}

}