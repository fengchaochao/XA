package com.prj.biz.bean.leavingmessage;

import com.prj.biz.bean._base.BaseEntity;

/** 
 * 描述: 客服系统 实体类<br>
 * @author Liang
 * @date 2017-07-27
 * 
 */
public class LeavingMessage extends BaseEntity  
{

    private static final long	serialVersionUID	= 1L;

    //  
    private String id;
    // 用户类型 1:商家 2:消费者 3:消费商 4:代理商
    private String userType;
    // 用户id 
    private String userId;
    // 状态 1：未回复 2：已回复 
    private String status;
    // 回复时间 
    private String replyDate;
    // 留言时间 
    private String leavingmessageDate;
    // 留言内容 
    private String leavingmessageContent;
    // 回复内容 
    private String replyContent;
    // 用户名称（显示用）
    private String userName;
    //用户头像（显示用）
    private String personPhoto;

    public String getUserName() {
		return userName;
	}
	public void setUserName(String userName) {
		this.userName = userName;
	}
	public String getId() {
        return id;
    }
    public void setId(String id) {
        this.id = id;
    }


    public String getUserType() {
        return userType;
    }
    public void setUserType(String userType) {
        this.userType = userType;
    }


    public String getUserId() {
        return userId;
    }
    public void setUserId(String userId) {
        this.userId = userId;
    }


    public String getStatus() {
        return status;
    }
    public void setStatus(String status) {
        this.status = status;
    }


    public String getReplyDate() {
		return replyDate;
	}
	public void setReplyDate(String replyDate) {
		this.replyDate = replyDate;
	}
	public String getLeavingmessageDate() {
		return leavingmessageDate;
	}
	public void setLeavingmessageDate(String leavingmessageDate) {
		this.leavingmessageDate = leavingmessageDate;
	}
	public String getLeavingmessageContent() {
        return leavingmessageContent;
    }
    public void setLeavingmessageContent(String leavingmessageContent) {
        this.leavingmessageContent = leavingmessageContent;
    }


    public String getReplyContent() {
        return replyContent;
    }
    public void setReplyContent(String replyContent) {
        this.replyContent = replyContent;
    }
	public String getPersonPhoto() {
		return personPhoto;
	}
	public void setPersonPhoto(String personPhoto) {
		this.personPhoto = personPhoto;
	}

}
