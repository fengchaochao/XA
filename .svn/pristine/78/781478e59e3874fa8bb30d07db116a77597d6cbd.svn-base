package com.prj.biz.enums;

/** 
* @Description: 发票流水状态
* @date 2016年4月21日 
* @author dfsoft
*/
public enum InvoicestatusEnum {

	INVOICESTATUS_UNAUDITED("1","待审核"),
	INVOICESTATUS_UNMAILED("2","已开票未邮寄"),
	INVOICESTATUS_MAILED("3","已邮寄"),
	INVOICESTATUS_SUCCESS("4","已完成"),
	INVOICESTATUS_REVOKE("5","作废");
	
	private String code;
	private String desc;
	
	InvoicestatusEnum(String code,String desc){
		this.code = code;
		this.desc = desc;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getDesc() {
		return desc;
	}

	public void setDesc(String desc) {
		this.desc = desc;
	}
	
	
}
