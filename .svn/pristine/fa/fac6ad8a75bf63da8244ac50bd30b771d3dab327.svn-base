}

#main #sidebar ul li.transaction-inquiry .active i {
  background-position: -60px -32px;
}

#main #sidebar ul li.business-reconciliation .active i {
  background-position: -100px -32px;
}

#main #sidebar ul li.ad-settings .active i {
