/* reset    begin */
a {
  outline: none;
  text-decoration: none;
}

　
a:active {
  background-color: transparent;
}

a:active, a:hover {
  outline: 0 none;
}

a:focus {
  outline: none;
}
ul, ol,li {
  list-style-type: none;
  list-style-image: none;
  padding: 0;
}

.clearfix:before,
.clearfix:after {
  content: "";
  display: table;
}

.clearfix:after {
  clear: both;
}

.clearfix {
  *zoom: 1;
}

/*reset end*/
.placehoderClass {
  color: #999;
}

.textOverflow {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  word-wrap: break-word;
  display: inline-block;
}

.selectmodel {
  position: relative;
  display: block;
  width: 100%;
}

.selectmodel .select {
  display: block;
  position: relative;
  height: 30px;
  line-height: 30px;
  padding: 0 14px;
  cursor: pointer;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.selectmodel .select:after {
  content: '';
  position: absolute;
  top: 40%;
  right: 6px;
  display: block;
  width: 10px;
  height: 6px;
  background: url(../img/select-pic.png) no-repeat center;
}

.selectmodel .selectbox {
  display: none;
  width: 100%;
  z-index: 999;
  position: absolute;
  top: 30px;
  left: -1px;
  border: 1px solid #e6e7ec;
}

.selectmodel .selectbox li {
  height: 30px;
  padding: 0 14px;
  line-height: 30px;
  background: #fff;
  cursor: pointer;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.selectmodel .selectbox li.active {
  background: #e6e7ec;
}

.selectmodel .selectbox li.hover {
  background: #f5f5f5;
}

.page {
  float: right;
  margin-top: 10px;
  font-size: 1.4rem;
  color: #414141;
}

.page a {
  float: left;
  display: inline-block;
  width: 70px;
  height: 30px;
  border: 1px solid #e6e7ec;
  text-align: center;
  line-height: 30px;
  -moz-border-radius: 4px;
  -webkit-border-radius: 4px;
  -ms-border-radius: 4px;
  border-radius: 4px;
}

.page a.indexpage {
  margin-right: 5px;
}

.page a.endpage {
  margin-left: 5px;
  margin-right: 10px;
}

.page a.compage {
  width: 30px;
  height: 30px;
}

.page a:hover {
  background: #e6e7ec;
}

.page span {
  float: left;
  display: block;
  height: 32px;
  padding: 0 8px;
  line-height: 32px;
}

.page .pageinp {
  float: left;
  display: block;
  width: 48px;
  height: 24px;
  padding: 3px;
  border: 1px solid #e6e7ec;
  margin-right: 5px;
  outline: none;
  text-align: center;
  -moz-border-radius: 4px;
  -webkit-border-radius: 4px;
  -ms-border-radius: 4px;
  border-radius: 4px;
}

.page .next {
  background: url(../img/next.png) no-repeat center;
}

.page .next:hover {
  background: #e6e7ec url(../img/next.png) no-repeat center;
}

.page .previous {
  background: url(../img/prev.png) no-repeat center;
}

.page .previous:hover {
  background: #e6e7ec url(../img/prev.png) no-repeat center;
}

body {
  background: #e6e7ec;
  font-family: '微软雅黑';
  font-weight: lighter;
}

#header {
  width: 100%;
  min-width: 1200px;
  height: 70px;
  background: #fff;
}

#header .header-main {
/*   width: 1200px; */
   width: 80%;
  margin: 0 auto;
}

#header .header-main .header-main-logo {
  float: left;
  height: 36px;
  padding: 17px 0;
}

#header .header-main .header-main-logo i {
  float: left;
  display: inline-block;
  width: 36px;
  height: 36px;
  background: url(../img/common-pic.png) no-repeat 0 2px;
}

#header .header-main .header-main-logo span {
  color: #414141;
  font-size: 22px;
}

#header .header-main-userinfo {
  float: right;
}

#header .header-main-userinfo span {
  float: left;
  display: inline-block;
  height: 70px;
  line-height: 70px;
  margin-right: 25px;
  color: #414141;
  font-size: 14px;
}

#header .header-main-userinfo a {
  display: inline-block;
  width: 84px;
  height: 70px;
  color: #4d98e5;
  background: #fff;
  text-align: center;
  line-height: 70px;
}

#header .header-main-userinfo a:hover {
  color: #fff;
  background: #4d98e5;
}

#main {
  width: 1200px;
  background: #36424f;
  margin: 20px auto 70px auto;
}

#main #sidebar {
  float: left;
  width: 210px;
}

#main #sidebar ul li a {
  position: relative;
  display: block;
  width: 166px;
  height: 32px;
  padding: 0 12px;
  line-height: 32px;
  color: #c0c4cd;
  font-size: 14px;
  cursor: pointer;
}

#mainMenu{
  padding-left: 30px;
}

#mainMenu>li>i+div{
  height: 40px;
  line-height: 40px;
  color:#fff;
}

#main #sidebar ul li a i {
  float: left;
  display: inline-block;
  width: 20px;
  height: 20px;
  margin-right: 13px;
  background: url(../img/common-pic.png) no-repeat;
}

#main #sidebar ul li a:hover {
  background: #4e5a67;
}

#main #sidebar ul li a.active {
  background: #4d98e5;
  color: #fff;
}

#main #sidebar ul li a.active:before {
  content: '';
  position: absolute;
  left: 0;
  top: 0;
  width: 4px;
  height: 36px;
  background: #fff;
}

#main #sidebar ul li.account-overview a i {
  background-position: 0 -32px;
}

#main #sidebar ul li.transaction-inquiry a i {
  background-position: -40px -32px;
}

#main #sidebar ul li.business-reconciliation a i {
